{
  "name": "@azureml/websockets",
  "version": "1.0.1",
  "license": "UNLICENSED",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "cross-env max-old-space-size=4096 NODE_ENV=production rollup -c",
    "lint": "run-p lint:*",
    "lint:style": "stylelint \"src/**/*.scss\" --allow-empty-input",
    "lint:ts": "tslint -p .",
    "lintfix": "run-p \"lint:* --fix\"",
    "start": "cross-env max-old-space-size=4096 rollup -c -w",
    "start-https": "cross-env max-old-space-size=4096 rollup -c -w",
    "test": "react-app-rewired test --coverage --passWithNoTests"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.ts?(x)",
      "!**/src/**/__data__/**/*.*",
      "!**/src/**/*.{test}.ts?(x)",
      "!**/src/setupTests.ts"
    ],
    "coverageReporters": [
      "cobertura",
      "lcov",
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@aspnet/signalr": "1.1.4",
    "@types/uuid": "^3.4.4",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "@aspnet/signalr": "1.1.4",
    "uuid": "^3.3.2"
  },
  "gitHead": "eea2ac4329bb7749af1982386926774066234a91"
}
