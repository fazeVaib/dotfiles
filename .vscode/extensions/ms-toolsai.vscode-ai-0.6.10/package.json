		"menus": {
			"commandPalette": [
				{
					"command": "vscodeai.selectSubscriptions",
					"when": "false"
				},
				{
					"command": "vscodeai.homepage.downloadSample",
					"when": "false"
				},
				{
					"command": "vscodeai.homepage.installDependentEnv",
					"when": "false"
				},
				{
					"command": "vscodeai.homepage.openLink",
					"when": "false"
				},
				{
					"command": "vscodeai.azureml.commandpalette.submit",
					"when": "!AzureMLRunExperimentInProgress"
				},
				{
					"command": "vscodeai.azureml.filecontextmenu.submitLast",
					"when": "!AzureMLRunExperimentInProgress && AzureMLLastRunAvailable"
				},
				{
					"command": "vscodeai.view.common.refresh",
					"when": "false"
				},
				{
					"command": "vscodeai.view.azureml.refresh",
					"when": "false"
				},
				{
					"command": "vscodeai.azureml.editjson.submitcodelens",
					"when": "AzureMLCodelensInProgress && !AzureMLRunExperimentInProgress"
				},
				{
					"command": "vscodeai.azureml.editjson.cancelcodelens",
					"when": "AzureMLCodelensInProgress && !AzureMLRunExperimentInProgress"
				},
				{
					"command": "vscodeai.azureml.editjson.submitruncodelens",
					"when": "AzureMLCodelensInProgress && AzureMLRunExperimentInProgress"
				},
				{
					"command": "vscodeai.azureml.editjson.cancelruncodelens",
					"when": "AzureMLCodelensInProgress && AzureMLRunExperimentInProgress"
				},
				{
					"command": "vscodeai.azureml.compute.submit",
					"when": "false"
				},
				{
					"command": "vscodeai.azureml.experiment.submit",
					"when": "false"
				},
				{
					"command": "vscodeai.azureml.runConfig.submit",
					"when": "false"
				},
				{
					"command": "vscodeai.azureml.toolbar.submit",
					"when": "false"
				},
				{
					"command": "vscodeai.azureml.filecontextmenu.submit",
					"when": "false"
				}
			],
			"explorer/context": [
				{
					"command": "vscodeai.azureml.filecontextmenu.submit",
					"when": "resourceLangId == python && !AzureMLRunExperimentInProgress"
				},
				{
					"command": "vscodeai.azureml.filecontextmenu.submitLast",
					"when": "resourceLangId == python && !AzureMLRunExperimentInProgress && AzureMLLastRunAvailable"
				}
			],
			"editor/context": [
				{
					"command": "vscodeai.azureml.filecontextmenu.submit",
					"when": "resourceLangId == python && !AzureMLRunExperimentInProgress"
				},
				{
					"command": "vscodeai.azureml.filecontextmenu.submitLast",
					"when": "resourceLangId == python && !AzureMLRunExperimentInProgress && AzureMLLastRunAvailable"
				}
			],
			"view/title": [
				{
					"command": "vscodeai.azureml.toolbar.submit",
					"when": "view == AzureMLExplorer && !AzureMLRunExperimentInProgress",
					"group": "navigation@1"
				},
				{
					"command": "vscodeai.azureml.workspace.add",
					"when": "view == AzureMLExplorer",
					"group": "navigation@2"
				},
				{
					"command": "vscodeai.view.azureml.refresh",
					"when": "view == AzureMLExplorer",
					"group": "navigation@3"
				}
			],
			"view/item/context": [
				{
					"command": "vscodeai.selectSubscriptions",
					"when": "view == AzureMLExplorer && viewItem == AzureMLSubscription",
					"group": "inline"
				},
				{
					"command": "vscodeai.view.azureml.refresh",
					"when": "view == AzureMLExplorer && !viewItem",
					"group": "general@4"
				},
				{
					"command": "vscodeai.azureml.workspace.add",
					"when": "view == AzureMLExplorer && viewItem == AzureMLSubscription",
					"group": "general@1"
				},
				{
					"command": "vscodeai.view.azureml.refresh",
					"when": "view == AzureMLExplorer && viewItem == AzureMLSubscription",
					"group": "general@4"
				},
				{
					"command": "vscodeai.azureml.workspace.remove",
					"when": "view == AzureMLExplorer && viewItem == AzureMLWorkspace",
					"group": "general@3"
				},
				{
					"command": "vscodeai.view.azureml.refresh",
					"when": "view == AzureMLExplorer && viewItem == AzureMLWorkspace",
					"group": "general@4"
				},
				{
					"command": "vscodeai.azureml.computeTarget.add",
					"when": "view == AzureMLExplorer && viewItem == AzureMLComputeRoot",
					"group": "general@1"
				},
				{
					"command": "vscodeai.azureml.computeTarget.attach",
					"when": "view == AzureMLExplorer && viewItem == AzureMLComputeRoot",
					"group": "general@2"
				},
				{
					"command": "vscodeai.view.azureml.refresh",
					"when": "view == AzureMLExplorer && viewItem == AzureMLComputeRoot",
					"group": "general@4"
				},
				{
					"command": "vscodeai.view.azureml.refresh",
					"when": "view == AzureMLExplorer && viewItem == AzureMLPipelineRoot",
					"group": "general@4"
				},
				{
					"command": "vscodeai.azureml.model.add",
					"when": "view == AzureMLExplorer && viewItem == AzureMLModelRoot",
					"group": "general@1"
				},
				{
					"command": "vscodeai.view.azureml.refresh",
					"when": "view == AzureMLExplorer && viewItem == AzureMLModelRoot",
					"group": "general@4"
				},
				{
					"command": "vscodeai.azureml.model.deployService",
					"when": "view == AzureMLExplorer && viewItem == AzureMLModel",
					"group": "general@4"
				},
				{
					"command": "vscodeai.azureml.model.remove",
					"when": "view == AzureMLExplorer && viewItem == AzureMLModel",
					"group": "general@3"
				},
				{
					"command": "vscodeai.azureml.model.download",
					"when": "view == AzureMLExplorer && viewItem == AzureMLModel",
					"group": "general@3"
				},
