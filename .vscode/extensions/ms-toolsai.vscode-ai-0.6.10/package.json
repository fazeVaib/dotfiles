{
	"name": "vscode-ai",
	"displayName": "Azure Machine Learning",
	"description": "Visual Studio Code extension for Azure Machine Learning",
	"version": "0.6.10",
	"publisher": "ms-toolsai",
	"engines": {
		"vscode": "^1.36.0"
	},
	"icon": "icons/logo.png",
	"preview": true,
	"categories": [
		"Azure",
		"Other"
	],
	"keywords": [
		"Azure Machine Learning",
		"AML",
		"Azure ML",
		"Deep Learning"
	],
	"repository": {
		"url": "https://github.com/Microsoft/vscode-tools-for-ai"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-tools-for-ai/issues",
		"email": "omscs@microsoft.com"
	},
	"extensionDependencies": [
		"ms-vscode.azure-account",
		"ms-python.python"
	],
	"activationEvents": [
		"onView:AzureMLExplorer",
		"onCommand:vscodeai.homepage.openInstallationPage",
		"onCommand:vscodeai.homepage.downloadSample",
		"onCommand:vscodeai.homepage.installDependentEnv",
		"onCommand:vscodeai.azureml.filecontextmenu.submit",
		"onCommand:vscodeai.azureml.filecontextmenu.submitLast",
		"onCommand:vscodeai.view.common.refresh",
		"onCommand:vscodeai.view.azureml.refresh",
		"onCommand:vscodeai.azureml.external.install",
		"onCommand:vscodeai.azureml.workspace.add",
		"onCommand:vscodeai.azureml.workspace.remove",
		"onCommand:vscodeai.azureml.computeTarget.add",
		"onCommand:vscodeai.azureml.computeTarget.attach",
		"onCommand:vscodeai.azureml.computeTarget.remove",
		"onCommand:vscodeai.azureml.computeTarget.detach",
		"onCommand:vscodeai.azureml.computeTarget.edit",
		"onCommand:vscodeai.azureml.computeTarget.viewProperties",
		"onCommand:vscodeai.azureml.model.add",
		"onCommand:vscodeai.azureml.model.deployService",
		"onCommand:vscodeai.azureml.model.remove",
		"onCommand:vscodeai.azureml.model.download",
		"onCommand:vscodeai.azureml.model.viewProperties",
		"onCommand:vscodeai.azureml.service.add",
		"onCommand:vscodeai.azureml.service.edit",
		"onCommand:vscodeai.azureml.service.remove",
		"onCommand:vscodeai.azureml.service.viewProperties",
		"onCommand:vscodeai.azureml.experiment.add",
		"onCommand:vscodeai.azureml.experiment.attachProject",
		"onCommand:vscodeai.azureml.experiment.preview",
		"onCommand:vscodeai.azureml.pipeline.preview",
		"onCommand:vscodeai.azureml.runConfig.create",
		"onCommand:vscodeai.azureml.runConfig.edit",
		"onCommand:vscodeai.azureml.runConfig.remove",
		"onCommand:vscodeai.azureml.environment.create",
		"onCommand:vscodeai.azureml.environment.view",
		"onCommand:vscodeai.azureml.environment.edit",
		"onCommand:vscodeai.azureml.commandpalette.submit",
		"onCommand:vscodeai.azureml.compute.submit",
		"onCommand:vscodeai.azureml.experiment.submit",
		"onCommand:vscodeai.azureml.runConfig.submit",
		"onCommand:vscodeai.azureml.toolbar.submit",
		"onCommand:vscodeai.feedback.issue",
		"onCommand:vscodeai.feedback.proactive",
		"onCommand:vscodeai.azureml.notebookVM.viewProperties"
	],
	"main": "./out/src/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "%configuration.title%",
			"properties": {
				"ai.homepage.openOnStartup": {
					"description": "%ai.homepage.openOnStartup.description%",
					"type": "boolean",
					"default": false
				},
				"ai.logger.level.file": {
					"description": "The level of message in the log file",
					"type": "string",
					"enum": [
						"error",
						"warn",
						"info",
						"verbose",
						"debug",
						"silly"
					],
					"default": "debug"
				},
				"ai.logger.level.channel": {
					"description": "The level of message in VS Code output pane",
					"type": "string",
					"enum": [
						"error",
						"warn",
						"info",
						"verbose",
						"debug",
						"silly"
					],
					"default": "info"
				},
				"ai.logger.level.window": {
					"description": "The level of message in VS Code notification window",
					"type": "string",
					"enum": [
						"error",
						"warn",
						"info",
						"verbose",
						"debug",
						"silly"
					],
					"default": "info"
				},
				"ai.telemetry.enable": {
					"type": "boolean",
					"default": true,
					"description": "%ai.telemetry.enable.description%"
				},
				"ai.azureml.showProgress": {
					"type": "boolean",
					"default": false,
					"description": "%ai.azureml.showProgress.description%"
				},
				"ai.webserver.startPort": {
					"type": "number",
					"default": 21000,
					"description": "%ai.webserver.startPort.description%"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "azure",
					"title": "Azure",
					"icon": "icons/azure.svg",
					"when": "!azuremlDisable"
				}
			]
		},
		"views": {
			"azure": [
				{
					"id": "AzureMLExplorer",
					"name": "Machine Learning",
					"when": "!azuremlDisable"
				}
			]
		},
