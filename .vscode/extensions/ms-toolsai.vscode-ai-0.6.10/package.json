	"scripts": {
		"vscode:prepublish": "cross-env NODE_ENV=production webpack --mode production --info-verbosity none",
		"build": "webpack --mode development --info-verbosity none",
		"watch": "webpack -w --mode development --info-verbosity none",
		"package": "vsce package",
		"tslint": "tslint --project ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"buildFunctionTests": "tsc -p ./ && node ./tools/rmdir.js ./out/test/smokeTest && node ./tools/rmdir.js ./out/test/unitTest && node ./tools/copy.js src/test/testFile/function_test.js node_modules/vscode/bin/test.js && babel --presets es2015 -d node_modules/@azureml/ node_modules/@azureml",
		"buildSmokeTests": "tsc -p ./ && node ./tools/rmdir.js ./out/test/functionTest && node ./tools/rmdir.js ./out/test/unitTest && node ./tools/copydir.js ./src/test/smokeTest/MNIST ./out/test/smokeTest/MNIST && node ./tools/copy.js src/test/testFile/smoke_test.js node_modules/vscode/bin/test.js && babel --presets es2015 -d node_modules/@azureml/ node_modules/@azureml",
		"runTests": "node ./node_modules/vscode/bin/test.js",
		"compile": "tsc -p ./",
		"unitTest": "tsc -p ./ && mocha --opts ./src/test/unitTest/mocha.unittests.opts"
	},
